    "@rollup/plugin-replace": "^2.4.1",
    "@wessberg/rollup-plugin-ts": "^1.3.11",
    "cross-env": "^7.0.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.42.4",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.31.0",
    "typescript": "^4.2.3"
